// MariFX Grading
hardwareshader postprocess scene
{
	Name "marifx_grading"
	Shader "shaders/glsl/marifx_grading.fp" 330
	Uniform vec3 grademul
	Uniform vec3 gradepow
	Uniform vec3 gradecol
	Uniform float gradecolfact
	Uniform float gradesatmul
	Uniform float gradesatpow
	Uniform float gradevalmul
	Uniform float gradevalpow
}

hardwareshader postprocess scene
{
	Name "insanity1"
	Shader "shaders/glsl/insanity_layer1.fp" 330
	Uniform float dfact
	Uniform float timer
	Texture fluidtex "textures/fluidtex.png"
}

hardwareshader postprocess scene
{
	Name "insanity2"
	Shader "shaders/glsl/insanity_layer2.fp" 330
	Uniform float tun
	Uniform float dfact
	Uniform float timer
	Texture warptex "textures/warptex.png"
}

